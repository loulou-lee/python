# 키보드에서 부서번호를 입력받아 해당 부서 직원자료(사번,이름,부서,연봉,직급) 출력
import MySQLdb

config = {
    'host':'127.0.0.1',
    'user':'root',
    'password':'123',
    'database':'test',
    'port':3306,
    'charset':'utf8',
    'use_unicode':True
}

def start():
    conn = MySQLdb.connect(**config)
    print(conn)

if __name__ == '__main__':
    start()